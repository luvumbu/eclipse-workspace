package exercice1;

import java.util.Scanner;

public class Exercice1 {

	
	
	public void infoexo () 
	{
		System.out.println("Exercice Algorithme : les Boucles");
		System.out.println("Ecrire un algorithme qui demande successivement 20 nombres à l’utilisateur"
				+ ", et qui lui dise ensuite quel était le plus "
				+ "grand parmi ces 20 nombres :");		
		//Une variable vide
		String prenom = "ndenga";		
		int taille  = prenom.length() ; 
	    //System.out.println(taille);		
		//System.out.println(prenom.substring(5,6));
		
		
		
		
		
		try {
			int nombre = Integer.parseInt(prenom);
			}
			catch(Exception e) {
			 System.out.println("pas bon");
			}
		for(int  i = 0 ; i<taille ; i++)
		{
			System.out.println(prenom.substring(i,i+1));
		}
		
		 
	}

}